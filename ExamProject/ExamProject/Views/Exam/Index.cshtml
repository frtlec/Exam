@model ExamProject.Dtos.WiredModel
@{
    ViewData["Title"] = "Index";
}

<style>
    .row{
        color:black;
        margin-bottom:25px;
        background-color:#eaeaea;
        padding:20px 0;
    }

</style>


@foreach (var item in Model.wiredItems)
{
   
   

   
        <a class="row" asp-action="ExamCreate" asp-controller="Exam" asp-route-id="@item.wiredId">

            <div class="col-sm-11">
                <h3>@Html.Raw(item.header)</h3>
            </div>
            <div class="col-sm-2">
                <i class="glyphicon glyphicon-chevron-right"></i>
            </div>
        </a>
   

}
